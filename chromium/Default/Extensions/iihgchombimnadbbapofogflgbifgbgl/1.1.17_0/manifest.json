{
   "background": {
      "persistent": false,
      "scripts": [ "background/index.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon/icon19.png",
         "38": "icon/icon38.png"
      },
      "default_title": "牛客简历助手"
   },
   "content_scripts": [ {
      "js": [ "content/index.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "简历一次填写，N网闪填，适配市面80%以上招聘网站，节省70%以上的简历重复填写时间。",
   "icons": {
      "128": "icon/icon128.png",
      "16": "icon/icon16.png",
      "48": "icon/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "牛客简历助手",
   "permissions": [ "alarms", "cookies", "storage", "notifications", "activeTab", "tabs", "alarms", "http://*/*", "https://*/*" ],
   "short_name": "简历助手",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.17",
   "web_accessible_resources": [ "content/*", "icon/*" ]
}
